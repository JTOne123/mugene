{
	"name": "vscode-language-mugene",
	"description": "language server for mugene MML compiler",
	"author": "atsushieno",
	"license": "MIT",
	"version": "0.2.1",
	"publisher": "atsushieno",
	"repository": {
		"type": "git",
		"url": "https://github.com/atsushieno/mugene"
	},
	"engines": {
		"vscode": "^1.23.0"
	},
	"scripts": {
		"postinstall": "cd client && npm install && cd ..",
		"compile": "npm run compile:client && npm run compile:server",
		"compile:client": "npm run prebuild && tsc -p client/tsconfig.json",
		"compile:server": "cd client/csharp && npm run compile && cd ../..",
		"prebuild": "mkdir -p ./client/out/tools/mugene/ && cp -R ../mugene/bin/Debug/* ./client/out/tools/mugene/",
		"watch:client": "npm run prebuild && tsc -w -p client/tsconfig.json"
	},
	"devDependencies": {
		"@types/mocha": "^5.2.6",
		"@types/node": "^11.13.8",
		"@types/rx-lite": "^4.0.6",
		"typescript": "^3.4.5",
		"vscode": "^1.1.33"
	},
	"dependencies": {
		"rx-lite": "^4.0.8"
	}
}
